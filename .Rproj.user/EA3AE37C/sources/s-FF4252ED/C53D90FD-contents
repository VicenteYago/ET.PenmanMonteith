% Please edit documentation in R/et0.R
\name{et0}
\alias{et0}
\title{Get daily Penman-Monteith Evapotranspiration from hourly data}
\usage{
et0(dates, temp, hr, uz, rs, lat, elev, crop = "short")
}
\arguments{
\item{dates}{POSIXct dates vector.}

\item{temp}{numeric vector of temperature in Celsius scale.}

\item{hr}{numeric vector of relative humidity (\%).}

\item{uz}{numeric vector of wind speed in m/s.}

\item{rs}{numeric vector of solar irradiation in W/m2}

\item{lat}{latitude.}

\item{elev}{numeric elevation in mts.}

\item{crop}{character "short" (default) of "tall".}
}
\value{
dataframe with columns \code{date} and \code{et0} containing the daily Penman-Monteith Reference Evapotranspiration
}
\description{
Get daily Penman-Monteith Evapotranspiration from hourly data
}
